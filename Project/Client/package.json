		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.5",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/libre-baskerville": "^4.5.10",
		"@fontsource/nixie-one": "^4.5.10",
		"@fontsource/space-mono": "^4.5.12",
		"axios": "^1.3.1",
		"d3": "^7.8.2",
		"svelte-d3-cloud": "^0.0.2",
		"svelte-speedometer": "^1.1.0",
		"svelte-typewriter": "^3.1.4"
	}
}
